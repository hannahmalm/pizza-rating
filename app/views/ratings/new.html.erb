<%= form_for(@rating) do |f|%>
    <%= f.label :title%>
    <%= f.text_field :title%>

    <%= f.label :rate%>
    <%= f.text_field :rate%>

    <%= f.label :comment%>
    <%= f.text_field :comment%>

    <%= f.label :url%>
    <%= f.text_field :url%>

    <%= f.hidden_field :pizza_id %>
    <%= f.submit "Submit Rating"%>


<%end%>