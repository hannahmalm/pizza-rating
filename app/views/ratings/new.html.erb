
<h1>Rate this Pizza</h1>
<%= form_for(@rating) do |f|%>
    <br>
        <%= f.label :title%>
        <%= f.text_field :title%>
     <br>
        <%= f.label :rate%>
        <%= f.text_field :rate%>
    <br>
        <%= f.label :comment%>
        <%= f.text_field :comment%>
    <br>
        <%= f.label :url%>
        <%= f.text_field :url%>
    <br>
        <%= f.hidden_field :pizza_id %>
        <%= f.submit "Submit Rating"%>
<%end%>