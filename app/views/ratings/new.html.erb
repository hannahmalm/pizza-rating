<%= form_for(@rating) do |f|%>
     <%= f.label :rate%>
    <%= f.text_field :rate%>

    <%= f.label :comment%>
    <%= f.password_field :comment%>

     <%= f.label :title%>
    <%= f.password_field :title%>

     <%= f.label :url%>
    <%= f.password_field :url%>

    <%= f.submit "Log In"%>


<%end%>